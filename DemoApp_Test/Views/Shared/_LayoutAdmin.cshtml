    <script>
        $(document).ready(function() {
            // Polyfill cho các hàm parse
            if (typeof parsedouble === 'undefined') {
                window.parsedouble = function(value) {
                    return parseFloat(value) || 0;
                };
            }

            // Fix cho chart
            if (typeof Chart !== 'undefined') {
                const originalGetRelativePosition = Chart.helpers.getRelativePosition;
                Chart.helpers.getRelativePosition = function(evt, chart) {
                    try {
                        return originalGetRelativePosition(evt, chart);
                    } catch (e) {
                        const rect = chart.canvas.getBoundingClientRect();
                        const x = evt.clientX - rect.left;
                        const y = evt.clientY - rect.top;
                        return {
                            x: x,
                            y: y
                        };
                    }
                };
            }
        });
    </script>

